{
  "name": "jason",
  "id": "jason",
  "version": "1.0.0",
  "description": "Fetch user risk score from jason portal and perform actions on users.",
  "type": ["user"],
  "netskope": false,
  "configuration": [
    {
      "label": "Base URL",
      "key": "url",
      "type": "text",
      "default": "https://us-api.jason.com",
      "mandatory": true,
      "description": "jason API base URL including region."
    },
    {
      "label": "Application ID",
      "key": "app_id",
      "type": "text",
      "mandatory": true,
      "description": "jason API Application ID."
    },
    {
      "label": "Application Key",
      "key": "app_key",
      "type": "password",
      "mandatory": true,
      "description": "jason API Application Key."
    },
    {
      "label": "Access Key",
      "key": "access_key",
      "type": "password",
      "mandatory": true,
      "description": "jason user's access key."
    },
    {
      "label": "Secret Key",
      "key": "secret_key",
      "type": "password",
      "mandatory": true,
      "description": "jason user's secret key."
    }
  ]
}
